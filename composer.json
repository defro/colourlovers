        }
    },
    "autoload-dev": {
        "psr-4": {
            "Defro\\ColourLovers\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "JoÃ«l Gaujard",
            "email": "j.gaujard@gmail.com",
            "role": "Developer",
            "homepage": "https://defro.github.io"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7.5"
